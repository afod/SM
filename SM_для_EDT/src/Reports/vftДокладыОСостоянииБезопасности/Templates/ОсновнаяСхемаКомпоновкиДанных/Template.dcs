////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	vftДокладОСостоянииБезопасности.Ссылка,
	vftДокладОСостоянииБезопасности.Дата,
	vftДокладОСостоянииБезопасности.Судно,
	vftДокладОСостоянииБезопасности.Статус,
	vftДокладОСостоянииБезопасности.ПериодОбзораС,
	vftДокладОСостоянииБезопасности.ПериодОбзораПо
ПОМЕСТИТЬ ВТ_Документы
ИЗ
	Документ.vftДокладОСостоянииБезопасности КАК vftДокладОСостоянииБезопасности
ГДЕ
	НЕ vftДокладОСостоянииБезопасности.ПометкаУдаления
	И vftДокладОСостоянииБезопасности.Статус &lt;&gt; ЗНАЧЕНИЕ(Перечисление.vftСтатусыДокументовСообщений.Черновик)
	И vftДокладОСостоянииБезопасности.Дата МЕЖДУ НАЧАЛОПЕРИОДА(&amp;Год, ГОД) И КОНЕЦПЕРИОДА(КОНЕЦПЕРИОДА(&amp;Год, ГОД), ДЕНЬ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Месяца.Месяц,
	ВЫБОР
		КОГДА ВТ_Документы.Ссылка ЕСТЬ NULL 
			ТОГДА 0
		ИНАЧЕ 1
	КОНЕЦ КАК ЕжемесячныйОтчет,
	ЕСТЬNULL(ВТ_Документы.Судно, ЗНАЧЕНИЕ(Справочник.vftСуда.ПустаяСсылка)) КАК СУДНО,
	ЕСТЬNULL(ВТ_Документы.Ссылка, ЗНАЧЕНИЕ(Документ.vftДокладОСостоянииБезопасности.ПустаяСсылка)) КАК Документ,
	ВЫБОР
		КОГДА ВТ_Документы.Ссылка ЕСТЬ NULL 
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА ВТ_Документы.Статус = ЗНАЧЕНИЕ(Перечисление.vftСтатусыДокументовСообщений.Получен)
						ИЛИ ВТ_Документы.Статус = ЗНАЧЕНИЕ(Перечисление.vftСтатусыДокументовСообщений.Закрыт)
					ТОГДА 1
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК ОтветКомпании
ИЗ
	ВТ_Месяца КАК ВТ_Месяца
		ПОЛНОЕ СОЕДИНЕНИЕ ВТ_Документы КАК ВТ_Документы
		ПО (МЕСЯЦ(ВТ_Документы.ПериодОбзораПо) = МЕСЯЦ(ВТ_Месяца.Месяц))</query>
	</dataSet>
